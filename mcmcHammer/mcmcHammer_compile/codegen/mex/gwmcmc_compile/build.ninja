build $builddir/_coder_gwmcmc_compile_api.o : cxx $root/interface/_coder_gwmcmc_compile_api.cpp
build $builddir/_coder_gwmcmc_compile_mex.o : cxx $root/interface/_coder_gwmcmc_compile_mex.cpp
build $builddir/eml_int_forloop_overflow_check.o : cxx $root/eml_int_forloop_overflow_check.cpp
