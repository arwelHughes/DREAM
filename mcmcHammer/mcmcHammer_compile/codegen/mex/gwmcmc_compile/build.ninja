# Link
build $root/gwmcmc_compile_mex.mexmaci64 : linkxx $builddir/gwmcmc_compile_data.o $builddir/rt_nonfinite.o $builddir/gwmcmc_compile_initialize.o $builddir/gwmcmc_compile_terminate.o $builddir/gwmcmc_compile.o $builddir/randn.o $builddir/bsxfun.o $builddir/reflectivity_calculation_wrapper.o $builddir/standardTF_reflectivityCalculation.o $builddir/standardTF_stanLay_reflectivityCalculation.o $builddir/backSort.o $builddir/allocateLayersForContrast.o $builddir/standardTF_layers_core.o $builddir/groupLayers_Mod.o $builddir/indexShapeCheck.o $builddir/makeSLDProfiles.o $builddir/sum.o $builddir/asymconvstep.o $builddir/applyScalarFunction.o $builddir/resampleLayers.o $builddir/adaptive_new.o $builddir/linspace.o $builddir/find.o $builddir/repmat.o $builddir/any1.o $builddir/sortLE.o $builddir/shiftdata.o $builddir/callReflectivity.o $builddir/colon.o $builddir/data_resolution_polly.o $builddir/resolution_polly.o $builddir/chiSquared.o $builddir/standardTF_stanlay_paraPoints.o $builddir/abeles_paraPoints.o $builddir/standardTF_custLay_reflectivityCalculation.o $builddir/standardTF_custlay_single.o $builddir/loopCppCustlayWrapper_CustLaysingle.o $builddir/rmmissing.o $builddir/standardTF_custlay_paraPoints.o $builddir/loopCppCustlayWrapper_CustLaypoints.o $builddir/standardTF_custXY_reflectivityCalculation.o $builddir/standardTF_custXY_single.o $builddir/loopCppCustlayWrapper_XYSingle.o $builddir/standardTF_custXY_paraPoints.o $builddir/loopCppCustlayWrapper_XYPoints.o $builddir/all.o $builddir/textOut.o $builddir/rand.o $builddir/nullAssignment.o $builddir/_coder_gwmcmc_compile_api.o $builddir/_coder_gwmcmc_compile_mex.o $builddir/eml_int_forloop_overflow_check.o $builddir/strcmp.o $builddir/sumMatrixIncludeNaN.o $builddir/acos.o $builddir/sqrt.o $builddir/sortrows.o $builddir/exp.o $builddir/eml_mtimes_helper.o $builddir/div.o $builddir/unsafeSxfun.o $builddir/gwmcmc_compile_mexutil.o $builddir/_coder_gwmcmc_compile_info.o $builddir/cpp_mexapi_version.o 
